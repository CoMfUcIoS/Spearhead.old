{
  "name": "charybdis",
  "version": "1.0.0",
  "description": "charybdis web react like app",
  "main": "src/server.js",
  "scripts": {
    "start": "yarn run build && NODE_ENV=production node_modules/.bin/babel-node src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-devSockets": "NODE_ENV=production babel-watch src/devSockets.js",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "author": "Ioannis Karasavvaidis",
  "license": "",
  "dependencies": {
    "history": "^4.5.1",
    "inferno-component": "^1.2.2",
    "inferno-router": "^1.2.2",
    "inferno-server": "^1.2.2",
    "inferno": "^1.2.2",
    "socket.io-client": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-inferno": "^1.7.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.22.0",
    "babel-watch": "^2.0.5",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.1",
    "eslint-plugin-inferno": "^6.9.2",
    "eslint": "^3.14.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "node-sass": "^4.4.0",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.7.2",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^2.6.1",
    "webpack": "^1.13.2"
  }
}
