{
  "author": "Ioannis Karasavvaidis",
  "bin": {
    "spearhead": "spearhead.js"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "dbus-native": "^0.2.0",
    "express": "^4.14.0",
    "history": "^4.5.1",
    "http-proxy": "^1.16.2",
    "http-server": "^0.9.0",
    "inferno": "^1.2.2",
    "inferno-compat": "^1.3.0-rc.3",
    "inferno-component": "^1.2.2",
    "inferno-router": "^1.2.2",
    "inferno-server": "^1.2.2",
    "jquery-deferred": "^0.3.1",
    "letsencrypt-express": "^2.0.6",
    "lodash": "^4.17.4",
    "monitor": "^0.6.10",
    "pmx": "^1.0.1",
    "request": "^2.79.0",
    "socket.io-client": "^1.7.2",
    "spdy": "^3.4.4",
    "uuid": "^3.0.1",
    "websocket": "^1.0.24"
  },
  "description": "Ecosystem of the gods",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-node-debug": "^2.0.0",
    "babel-plugin-inferno": "^1.7.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.1",
    "eslint": "^3.14.1",
    "eslint-plugin-inferno": "^6.9.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.7.2",
    "style-loader": "^0.13.1",
    "stylelint-config-sass-guidelines": "^2.0.0",
