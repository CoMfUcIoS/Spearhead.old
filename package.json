  "license": "CC-BY-NC-ND-4.0",
  "main": "spearhead",
  "name": "spearhead",
  "scripts": {
    "buildDocs": "yuidoc ./ -o docs",
    "cerberus": "authbind --deep pm2 start ./cerberus/index.js --name cerberus --interpreter './node_modules/babel-cli/bin/babel-node.js' --watch true",
    "charybdis": "npm run charybdis:build && npm run charybdis:run",
    "charybdis:build": "cd charybdis/ && NODE_ENV=production webpack -p && cd ../",
    "charybdis:run": "NODE_ENV=production pm2 start ./charybdis/src/server.js --name charybdis --interpreter './node_modules/babel-cli/bin/babel-node.js' --watch true",
    "charybdis:dev": "npm run charybdis:dev:build && babel-node --inspect ./charybdis/src/server.js",
    "charybdis:dev:build": "cd charybdis/ && webpack --progress && cd ../",
    "charybdis:single": "http-server ./charybdis/src/static",
    "hydra": "pm2 start ./hydra/index.js --name hydra --interpreter './node_modules/babel-cli/bin/babel-node.js' --watch true",
    "kill": "pm2 kill",
    "list": "pm2 l",
    "medusa": "pm2 start ./medusa/index.js --name medusa --interpreter './node_modules/babel-cli/bin/babel-node.js' --watch true",
    "start": "npm run hydra && npm run cerberus && npm run medusa && npm run charybdis"
  },
  "version": "1.0.1"
}
